{
  "name": "@shakuroinc/figma-extractor",
  "version": "3.1.0",
  "author": "Shakuro team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shakurocom/figma-extractor"
  },
  "bin": {
    "figma-extract": "./dist/bin.js"
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "index.d.ts"
  ],
  "scripts": {
    "dev": "ts-node ./src/bin.ts",
    "build": "rm -rf dist && yarn tsc",
    "lint": "eslint . --ext .ts,.tsx,.js --ignore-path .gitignore",
    "test": "jest",
    "prepare": "husky install",
    "release": "yarn build && git push --tags && npm publish --access public --new-version"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "cosmiconfig": "^8.0.0",
    "figma-js": "^1.16.0",
    "shelljs": "^0.8.5",
    "svg-sprite-generator": "^0.0.7",
    "svgo": "^3.0.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@shakuroinc/eslint-config-base": "1.0.5",
    "@types/jest": "^29.2.4",
    "@types/shelljs": "^0.8.11",
    "@types/svgo": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "babel-jest": "^29.3.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^7.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "memfs": "^3.4.12",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && lint-staged"
    }
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.10"
  }
}
